{
  "name": "user-service",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "test": "bun run test:auth && bun run test:users",
    "test:auth-service": "bun test tests/services/auth.service.test.ts",
    "test:auth-routes": "bun test tests/routes/auth.route.test.ts",
    "test:auth": "bun run test:auth-service && bun run test:auth-routes",
    "test:users-service": "bun test tests/services/user.service.test.ts",
    "test:users-routes": "bun test tests/routes/user.route.test.ts",
    "test:stripe": "bun test tests/services/stripe-connect.service.test.ts",
    "test:users": "bun run test:users-service && bun run test:users-routes && bun run test:stripe"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.3.0",
    "@elysiajs/cors": "latest",
    "@elysiajs/jwt": "latest",
    "@elysiajs/swagger": "latest",
    "@neondatabase/serverless": "^0.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/stripe": "^8.0.417",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.44.4",
    "elysia": "latest",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "pg": "^8.16.3",
    "postgres": "latest",
    "stripe": "^18.4.0",
    "users-service": "."
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "bun-types": "^1.2.20",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.31.4",
    "vitest": "^3.2.4"
  },
  "module": "src/index.js"
}